
<div class="container">
  <div class="row">
    <% for(var i=0; i<meetups.length; i++) {%>
      <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
        <div class="meetup-card">
          <% var strlink= '/group/';
                strlink = strlink + meetups[i].id;
                %>
          <a href = <%= strlink %> class="nametag-card-name">
            <h3>
              <%= meetups[i].name %>
            </h3>
          </a>
        </div>
        <div class="meetups-info">
            <div class="meetup-members">150 <span>rockers</span></div>
            <div class="meetup-date">Meetup At: Dec 27</div>
        </div>
      </div>
    <% } %>
<div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
<div class="meetup-card-add">
  <a href = "/group/new" class="nametag-card-name-add">
    <h3>
      Start a new group!
    </h3>
  </a>
</div>
<div class="meetups-info">
    <div class="meetup-members"><span>rockers</span></div>
    <div class="meetup-date"></div>
</div>
</div>



  </div>
</div>