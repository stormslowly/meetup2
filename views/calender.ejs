<div class="container">

  <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10">

    <div id="meetup-day-group">
      <div class="meetup-date">
        Thirday JAN 1
      </div>
      <ul>
      <% for(var i=0; i<events.length; i++) {%>

        <li class="meetup-calender">

          <div class="meetup-time col-xs-3 col-sm-3 col-md-3 col-lg-3">
            <% var dd= events[i].date; %>
            <h5><%= dd.toLocaleDateString() %></h5>
            <h5><%= dd.getHours() %>:<%= dd.getMinutes() %>:<%= dd.getSeconds() %></h5>
          </div>
          <div class="col-xs-9 col-sm-9 col-md-9 col-lg-9">
            <div class="meetup-localtion"><%= events[i].address %></div>
            <div class="meetup-title">
            <% var strlink= '/showevent/';
                strlink = strlink + events[i].id;
                %>
            <a href= <%= strlink %> >
            <%= events[i].topic %></a></div>
            <div class="meetup-members">7 <span>hacks Going</span>
            </div>
          </div>
        </li>

      <% } %>

      </ul>
    </div>
  </div>
</div>

<script type="text/javascript">
  $(function  () {
    $('.meetup-date')
      .waypoint({handler:function(dir){
          console.log(this.element.addClass)
          if(dir==='down'){
          $(this.element).addClass('fixtop');
          }
          else{
            $(this.element).removeClass('fixtop');
          }
        }
      })
  })
</script>
